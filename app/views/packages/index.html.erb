<p id="notice"><%= notice %></p>

<h1>Packages</h1>

<table>
  <thead>
    <tr>
      <th>Is shipped</th>
      <th>Ship date</th>
      <th>Shipping company</th>
      <th>Charged</th>
      <th>Cost</th>
      <th>Fee</th>
      <th>Weight</th>
      <th>Size</th>
      <th>Tracking</th>
      <th>Notes</th>
      <th>Order</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @packages.each do |package| %>
      <tr>
        <td><%= package.is_shipped %></td>
        <td><%= package.ship_date %></td>
        <td><%= package.shipping_company %></td>
        <td><%= package.charged %></td>
        <td><%= package.cost %></td>
        <td><%= package.fee %></td>
        <td><%= package.weight %></td>
        <td><%= package.size %></td>
        <td><%= package.tracking %></td>
        <td><%= package.notes %></td>
        <td><%= package.order %></td>
        <td><%= link_to 'Show', package %></td>
        <td><%= link_to 'Edit', edit_package_path(package) %></td>
        <td><%= link_to 'Destroy', package, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Package', new_package_path %>
